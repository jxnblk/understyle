{
  "name": "understyle",
  "version": "1.0.0",
  "description": "Functional style utilities for authoring JavaScript style objects",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "mkdir -p dist && babel src --out-dir dist",
    "test": "standard && ava -v"
  },
  "keywords": [
    "style",
    "css",
    "css-in-js",
    "utility"
  ],
  "author": "Brent Jackson",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.5",
    "standard": "^7.1.2"
  },
  "dependencies": {
    "object-assign": "^4.1.0"
  },
  "ava": {
